      { "name": "INFLUXDB", "label": "INFLUXDB", "default": "telegraf" },
      { "name": "INFLUXDB_BIND_ADDRESS", "label": "INFLUXDB_BIND_ADDRESS", "default": "0.0.0.0:8088" }
    ],
    "volumes": [ { "container": "/var/lib/influxdb", "bind": "/data/influxdb" } ]
  },
